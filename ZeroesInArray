#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
        cin>>arr[i];
        
        int p=0;
	    for(int i=0;i<n;i++){
	        if(arr[i]!=0)
	            arr[p++]=arr[i];
	    }
	    while(p<n){
	          arr[p]=0;
	          p++;
	    }
        
        for(int i=0;i<n;i++){
	        cout<<arr[i]<<" ";
	    }
    return 0;
}
